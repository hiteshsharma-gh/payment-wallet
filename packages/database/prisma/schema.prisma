generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id        String      @unique @default(uuid())
  name      String?
  email     String      @unique
  password  String?
  verified  Boolean     @default(false)
  auth_type AuthType
  type      AccountType
}

enum AccountType {
  User
  Merchant
}

enum AuthType {
  Credentials
  Google
}
